@model IEnumerable<UserRoleResponseModel>;

<div class="page-header">
    <h1>List of User Roles</h1>
</div>

<a asp-action="Create" asp-controller="UserRole">Add New User Role</a>

<table class="table table-bordered table-striped table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>UserId</th>
            <th>RoleId</th>
            <th colspan="2">Options</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var m in Model)
            {
                <tr>
                    <td>@m.Id</td>
                    <td>@m.UserId</td>
                    <td>@m.RoleId</td>
                    <td><a asp-action="Edit" asp-controller="UserRole" asp-route-id="@m.Id">Edit</a></td>
                    <td><a asp-action="Delete" asp-controller="UserRole" asp-route-id="@m.Id">Delete</a></td>
                </tr>
            }
        }
    </tbody>
</table>